<li class="item__inner <%= data.tagEn %>" ontouchstart>
  <div class="item__contents">
    <p class="item__title-wrap">
      <span class="item__title"><%= data.title %></span>
    </p>
    <a href="#inline_<%= data.id %>" class="inline">
      <img src="../img/items/<%= data.id %>_<%= data.slug %>.<%= data.format %>" class="item__tmb" alt="<%= data.title %>" loading="lazy">
    </a>
    <div id="inline_<%= data.id %>" class="item__modal">
      <div class="item__headline">
        <h3 class="item__headline-title"><%= data.title %></h3>
      </div>
      <div class="item__modal-contents">
        <img src="../img/items/<%= data.id %>_<%= data.slug %>.<%= data.format %>" alt="<%= data.title %>" class="item__modal-img">
        <div class="item__modal-details-wrap">
          <div class="item__modal-btn-wrap">
            <p class="item__modal-btn" >
              <a href="../img/items/<%= data.id %>_<%= data.slug %>.<%= data.format %>" class="btn3" download>
                <span class="btn__inner">
                  <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M6 20L18 20" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                  <path d="M12 4V16M12 16L15.5 12.5M12 16L8.5 12.5" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                  </svg><strong>無料<br class="sp-only">サンプル</strong>
                </span>
              </a>
            </p>
            <p class="item__modal-btn" >
              <a href="<%= data.pxLink %>" class="btn3" target="_blank" rel="noopener">
                <span class="btn__inner"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M8 21H20.4C20.7314 21 21 20.7314 21 20.4V3.6C21 3.26863 20.7314 3 20.4 3H3.6C3.26863 3 3 3.26863 3 3.6V16" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M3.5 20.5L12 12M12 12V16M12 12H8" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>PIXTA</span>
              </a>
            </p>
            <p class="item__modal-btn" >
              <a href="<%= data.asLink %>" class="btn3" target="_blank" rel="noopener">
                <span class="btn__inner"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M8 21H20.4C20.7314 21 21 20.7314 21 20.4V3.6C21 3.26863 20.7314 3 20.4 3H3.6C3.26863 3 3 3.26863 3 3.6V16" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M3.5 20.5L12 12M12 12V16M12 12H8" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>Adobe <br class="sp-only">Stock</span>
              </a>
            </p>
            <p class="item__modal-btn">
              <a href="<%= data.ssLink %>" class="btn3" target="_blank" rel="noopener">
                <span class="btn__inner"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M8 21H20.4C20.7314 21 21 20.7314 21 20.4V3.6C21 3.26863 20.7314 3 20.4 3H3.6C3.26863 3 3 3.26863 3 3.6V16" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M3.5 20.5L12 12M12 12V16M12 12H8" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>Sutter<br class="sp-only">stock</span>
              </a>
            </p>
            <p class="item__modal-btn">
              <a href="<%= data.shopLink %>" class="btn3" target="_blank" rel="noopener">
                <span class="btn__inner"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M8 21H20.4C20.7314 21 21 20.7314 21 20.4V3.6C21 3.26863 20.7314 3 20.4 3H3.6C3.26863 3 3 3.26863 3 3.6V16" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M3.5 20.5L12 12M12 12V16M12 12H8" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>素材<br class="sp-only">ごよみ</span>
              </a>
            </p>
          </div>
        </div>
      </div>
      <p class="item__date">公開日：<%= data.date %></p>
    </div>
  </div>
</li>
